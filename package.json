  "bugs": {},
  "homepage": "",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.7.2",
    "postcss-cli": "^4.1.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "html-minifier": "^3.5.7",
    "uswds": "1.4.1"
  }
}
