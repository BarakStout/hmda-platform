{
  "name": "hmda-platform-api",
  "version": "0.0.0",
  "description": "Documentation for the HMDA Platform public API.",
  "main": "",
  "engines": {
    "node": ">=7.x"
  },
  "scripts": {
    "postinstall": "yarn run build",
    "build": "yarn run uswds:img && yarn run uswds:fonts && yarn run uswds:sass",
    "uswds:img": "cp -r ./node_modules/uswds/src/img/* ./img",
    "uswds:fonts": "cp -r ./node_modules/uswds/src/fonts/sourcesanspro* ./fonts",
    "uswds:sass": "cp -r ./node_modules/uswds/src/stylesheets/* ./_sass"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "HMDA",
    "API"
  ],
  "author": "Consumer Financial Protection Bureau",
  "license": "CC0-1.0",
  "bugs": {},
  "homepage": "",
  "devDependencies": {},
  "dependencies": {
    "uswds": "1.4.1"
  }
}
