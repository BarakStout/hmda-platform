{
  "name": "hmda-platform-api",
  "version": "0.0.0",
  "description": "Documentation for the HMDA Platform public API.",
  "main": "",
  "engines": {
    "node": ">=7.x"
  },
  "scripts": {
    "postinstall": "yarn run build",
    "build": "yarn run img && yarn run fonts",
    "img": "cp -r ./node_modules/uswds/dist/img/* ./img",
